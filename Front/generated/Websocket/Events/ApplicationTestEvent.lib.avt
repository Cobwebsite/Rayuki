import { MainEndPoint } from '../MainEndPoint.lib.avt';

namespace Websocket.Events {

	@Convertible()
	export class Body {
		public static get Fullname(): string { return "Core.Websocket.Events.ApplicationTestEvent+Body, Core"; }
		public id!: number;
		public name!: string;
	}

	export class ApplicationTestEvent extends AventusSharp.WebSocket.Event<Body> {
		/**
		 * @inheritdoc
		 */
		protected override path(): string {
			return `${this.getPrefix()}Core.Websocket.Events.ApplicationTestEvent`;
		}
		public constructor(endpoint?: AventusSharp.WebSocket.EndPoint, getPrefix?: () => string) {
			super(endpoint ?? MainEndPoint.getInstance(), getPrefix);
		}
	}

	export class ApplicationTestEvent2 extends AventusSharp.WebSocket.Event<string> {
		/**
		 * @inheritdoc
		 */
		protected override path(): string {
			return `${this.getPrefix()}/application/test/2`;
		}
	}

}