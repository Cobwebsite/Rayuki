namespace Errors {

	export enum CoreErrorCode {
		NotImplemented,
		NotAvailable,
		Impossible,
		NotAllowed,
		NotLogin,
		ConversionFailed,
		SeederError,
		MigrationError,
		UnknowError,
		TransactionGuidMissmatch
	}

	@Convertible()
	export class CoreError extends Aventus.GenericError<typeof CoreErrorCode>  {
		public static get Fullname(): string { return "Core.Tools.CoreError, Core"; }
	}

}